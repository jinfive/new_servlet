    <!--   서블릿 이름 경로 -->
    <servlet>
        <servlet-name>city2</servlet-name>
        <servlet-class>org.example.demo5.CityServlet2</servlet-class>
    </servlet>
    <!--   서블릿 이름에 맞는 url 매핑 -->
    <servlet-mapping>
        <servlet-name>city2</servlet-name>
        <url-pattern>/city2</url-pattern>
    </servlet-mapping>

    <!--   서블릿 이름 경로 -->
    <servlet>
        <servlet-name>player</servlet-name>
        <servlet-class>org.example.demo5.PlayerServlet</servlet-class>
    </servlet>
